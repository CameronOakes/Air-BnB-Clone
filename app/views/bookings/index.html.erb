<h1>Your Bookings</h1>

<% if @bookings && @bookings.any? %>
  <% @bookings.each do |booking| %>
    <div>
      <h2><%= booking.flat.address %></h2>
      <p>From: <%= booking.start_date %></p>
      <p>To: <%= booking.end_date %></p>
    </div>
  <% end %>
<% else %>
  <p>You have no bookings.</p>
<% end %>
